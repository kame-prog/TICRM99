@model TICRM.DTOs.WorkFlowMappingDTO

<div class="modal-header modal-header-rightside">
    <h5 class="modal-header-title" id="exampleModalLabel">WORKFLOWMAPPING DETAILS</h5>
    <button type="button" class="close modalrightsideClose" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">X</span>
    </button>
</div>
<div class="modal-body">
    <div class="tab-content">
        <div class="tab-pane active show" id="m_portlet_tab_Leads" role="tabpanel">

            <div class="row container">
                <table class="tb-horizontal">
                    <tr>
                        <th class="blockquote">@Html.DisplayNameFor(model => model.SourceType)</th>
                        <td class="blockquote">@Html.DisplayFor(model => model.SourceType)</td>
                    </tr>
                    <tr>
                        <th class="blockquote">@Html.DisplayNameFor(model => model.SourceValue)</th>
                        <td class="blockquote">@Html.DisplayFor(model => model.SourceValue)</td>
                    </tr>
                    <tr>
                        <th class="blockquote">@Html.DisplayNameFor(model => model.Action)</th>
                        <td class="blockquote">@Html.DisplayFor(model => model.Action)</td>
                    </tr>
                    <tr>
                        <th class="blockquote">@Html.DisplayNameFor(model => model.CreatedBy)</th>
                        <td class="blockquote">@Html.DisplayFor(model => model.CreatedBy)</td>
                    </tr>
                    <tr>
                        <th class="blockquote">@Html.DisplayNameFor(model => model.CreatedDate)</th>
                        <td class="blockquote">@Html.DisplayFor(model => model.CreatedDate)</td>
                    </tr>
                    <tr>
                        <th class="blockquote">@Html.DisplayNameFor(model => model.UpdatedBy)</th>
                        <td class="blockquote">@Html.DisplayFor(model => model.UpdatedBy)</td>
                    </tr>
                    <tr>
                        <th class="blockquote">@Html.DisplayNameFor(model => model.UpdatedDate)</th>
                        <td class="blockquote">@Html.DisplayFor(model => model.UpdatedDate)</td>
                    </tr>
                    <tr>
                        <th class="blockquote">@Html.DisplayNameFor(model => model.WorkFlow.Name)</th>
                        <td class="blockquote">@Html.DisplayFor(model => model.WorkFlow.Name)</td>
                    </tr>
                </table>
            </div>
        </div>

    </div>
</div>

<div class="modal-footer modal-rightside-footer">
    <a href="@Url.Action("Edit", "WorkFlowMappings" , new { id=Model.WorkFlowMappingId })" class="btn btn-primary m-btn m-btn--custom m-btn--icon m-btn--air"><i class="fa fa-pencil-alt"></i> &nbsp; Edit</a>
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
</div>
