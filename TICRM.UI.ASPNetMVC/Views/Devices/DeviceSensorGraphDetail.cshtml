@model TICRM.DTOs.DeviceSensorGraphDto

@{
    ViewBag.Title = "DeviceSensorGraphDetail";
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";

}

<style>
    .inp-bor {
        border: 1px solid #e8ebf3;
    }
</style>
<div class="page-wrapper">
    <!-- Page Content-->
    <div class="page-content-tab">
        <div class="container-fluid">
            <div class="form-group">
                <button type="button" class="btn " onclick="location.href='@Url.Action("CreateDeviceSensorGraph", "Devices")'"><i class="fa fa-plus-circle text-primary me-2"></i>New<i class="fa fa-angle-down ms-2"></i></button>
                <button type="button" class="btn" id="RefreshPage"><i class="fa fa-clock text-primary me-2"></i>Refresh<i class="fa fa-angle-down ms-2"></i></button>
            </div>
            <!-- Page-Title -->
            <div class="row">
                <div class="col-sm-12">
                    <div class="page-title-box">
                        <h4 class="page-title"><b>@Html.DisplayFor(model => model.DeviceName)</b></h4>
                    </div>
                    <div class="card">
                        <div class="card-body">

                            <div>
                                <!--end page-title-box-->
                                <!--end col-->

                                <div class="col-sm-12">
                                    <div class="tab-content" id="nav-tabContent">
                                        <div class="tab-pane active">
                                            <div class="row">
                                                <h4 class="card-title mt-3 mb-1"><span class="fa fa-info-circle" style="margin-left:25px">Device Sensor Graph Detail</span></h4>
                                                <br />
                                                <div class="mb-3 row">

                                                </div>
                                                <div class="col-md-6">
                                                    <div class="mb-3 row">
                                                        <label for="example-text-input" class="col-lg-2 col-md-3 col-sm-3 col-form-label text-end">Device<span class="text-danger">  *</span></label>
                                                        <div class="col-lg-10 col-md-9 col-sm-9">
                                                            <div class=" p-2 border bg-light">@Html.DisplayFor(model => model.DeviceName)</div>
                                                        </div>
                                                    </div>
                                                    <div class="mb-3 row">
                                                        <label for="example-text-input" class="col-lg-2 col-md-3 col-sm-3 col-form-label text-end">Sensor<span class="text-danger">  *</span></label>
                                                        <div class="col-lg-10 col-md-9 col-sm-9">
                                                            <div class=" p-2 border bg-light">@Html.DisplayFor(model => model.SensorName)</div>
                                                        </div>
                                                    </div>
                                                    <div class="mb-3 row">
                                                        <label for="example-text-input" class="col-lg-2 col-md-3 col-sm-3 col-form-label text-end">Channel<span class="text-danger">  *</span></label>
                                                        <div class="col-lg-10 col-md-9 col-sm-9">
                                                            <div class=" p-2 border bg-light">@Html.DisplayFor(model => model.Channel)</div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="mb-3 row">
                                                        <label for="example-text-input" class="col-lg-2 col-md-3 col-sm-3 col-form-label text-end">Graph<span class="text-danger">  *</span></label>
                                                        <div class="col-lg-10 col-md-9 col-sm-9">
                                                            <div class=" p-2 border bg-light">@Html.DisplayFor(model => model.GraphName)</div>
                                                        </div>
                                                    </div>


                                                    <div class="mb-3 row">
                                                        <label for="example-text-input" class="col-lg-2 col-md-3 col-sm-3 col-form-label text-end">Data Duration<span class="text-danger">  *</span></label>
                                                        <div class="col-lg-10 col-md-9 col-sm-9">
                                                            <div class=" p-2 border bg-light">@Html.DisplayFor(model => model.Data)</div>
                                                        </div>
                                                    </div>
                                                    <div class="mb-3 row">
                                                        <label for="example-text-input" class="col-lg-2 col-md-3 col-sm-3 col-form-label text-end">Network<span class="text-danger">  *</span></label>
                                                        <div class="col-lg-10 col-md-9 col-sm-9">
                                                            <div class=" p-2 border bg-light">@Html.DisplayFor(model => model.Network)</div>
                                                        </div>
                                                    </div>

                                                </div><!--end col-->
                                            </div><!--end row-->
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <a href="/Devices/DeviceSensorGraphList" class="btn btn-primary ">
                                                <i class="la la-arrow-left"></i>
                                                <span>Back to List</span>
                                            </a>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/TIjQuery/AccountDetailjQuery.js"></script>