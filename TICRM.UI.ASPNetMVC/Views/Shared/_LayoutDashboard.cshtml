    });


    $(document).ready(function () {
        // Get the current controller name from the URL
        var currentController = window.location.pathname.split('/')[1];

        // Add "active" class to the corresponding sidebar item based on the current controller
        if (currentController === "Accounts" || currentController === "Contact" || currentController === "CustomerAssets" || currentController === "Locations" || currentController === "Addresses") {
            $("#customer-tab").addClass("active");
            $("#customer").addClass("active");
            //Here we check side bar is show or not
            var HomeSideBarShow = $("#customerUI").hasClass("show");
            //If side bar show then we deactive the "customer" and "customer-tab"
            if (HomeSideBarShow) {
                $("#customer-tab").removeClass("active");
                $("#customer").removeClass("active");
            }
            
        } else if (currentController === "ServiceCalls" || currentController === "Activities" || currentController === "Leads" || currentController === "Opportunities") {
            $("#sales-tab").addClass("active");
            $("#sales").addClass("active");
            //Here we check side bar is show or not
            var HomeSideBarShow = $("#salesUI").hasClass("show");
            //If side bar show then we deactive the "sales" and "sales-tab"
            if (HomeSideBarShow) {
                $("#sales-tab").removeClass("active");
                $("#sales").removeClass("active");
            }
        } else if (currentController === "Cases" || currentController === "WorkOrders") {
            $("#Services-tab").addClass("active");
            $("#Services").addClass("active");
            //Here we check side bar is show or not
            var HomeSideBarShow = $("#ServicesUI").hasClass("show");
            //If side bar show then we deactive the "Services" and "Services-tab"
            if (HomeSideBarShow) {
                $("#Services-tab").removeClass("active");
                $("#Services").removeClass("active");
            }
        } else {
            //If these id no any side bar component is active then we active the "Home" side bar components.
            $("#Home").addClass("active")

        }
      
    });



</script>

<!--Java-Script End-->