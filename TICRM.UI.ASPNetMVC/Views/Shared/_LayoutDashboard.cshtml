    $(document).ready(function () {
        $('#myTable').DataTable();

        //Success Toast appear
        var data = '@TempData["Success"]';
        if (data.length != 0) {
            toastr.success(data);
        }
        //Warning Toast appear
        var Warning = '@TempData["Warning"]';
        if (Warning.length != 0) {
            toastr.warning(Warning);
        }
        //Exist Message popup
        var Exist = '@ViewBag.exist';
        if (Exist.length != 0) {
            swal.fire({
                title: "Exist",
                text: Exist,
                icon: "warning",
                button: "Ok",
            });
        }
         //Error Message popup
        var error = '@ViewBag.error';
        if (error.length != 0) {
            swal.fire({
                title: "Warning",
                text: error,
                icon: "warning",
                button: "Ok",
            });
        }
    });
    //Script for exporting table in .xls(Excel) file
        $("#btnExport").click(function () {
            $("#myTable").table2excel({
                filename: "Table.xls"
            });
        });
    //Script for refreshing the page
    $('#RefreshPage').click(function () {
        location.reload();
    });

